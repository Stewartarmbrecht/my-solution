{
  "name": "my-app-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/my-app-e2e/src",
  "projectType": "application",
  "targets": {
    "build-ios": {
      "executor": "@nx/detox:build",
      "options": {
        "detoxConfiguration": "ios.sim.local"
      },
      "configurations": {
        "bare": {
          "detoxConfiguration": "ios.sim.debug"
        },
        "production": {
          "detoxConfiguration": "ios.sim.release"
        }
      }
    },
    "test-ios": {
      "executor": "@nx/detox:test",
      "options": {
        "detoxConfiguration": "ios.sim.local",
        "buildTarget": "my-app-e2e:build-ios"
      },
      "configurations": {
        "bare": {
          "detoxConfiguration": "ios.sim.debug",
          "buildTarget": "my-app-e2e:build-ios:bare"
        },
        "production": {
          "detoxConfiguration": "ios.sim.release",
          "buildTarget": "my-app-e2e:build-ios:production"
        }
      }
    },
    "build-android": {
      "executor": "@nx/detox:build",
      "options": {
        "detoxConfiguration": "android.emu.local"
      },
      "configurations": {
        "bare": {
          "detoxConfiguration": "android.emu.debug"
        },
        "production": {
          "detoxConfiguration": "android.emu.release"
        },
        "genymotion": {
          "detoxConfiguration": "android.genycloud.debug"
        }
      }
    },
    "test-android": {
      "executor": "@nx/detox:test",
      "dependsOn": [
      ],
      "options": {
        "detoxConfiguration": "debug-genycloud",
        "buildTarget": "my-app-e2e:build-android"
      },
      "configurations": {
        "genymotion": {
          "detoxConfiguration": "android.genycloud.debug"
        },
        "debug-att": {
          "detoxConfiguration": "android.att.debug",
          "reuse": true
        },
        "bare": {
          "detoxConfiguration": "android.emu.debug",
          "buildTarget": "my-app-e2e:build-android:bare"
        },
        "production": {
          "detoxConfiguration": "android.emu.release",
          "buildTarget": "my-app-e2e:build-android:production"
        },
        "production-att": {
          "detoxConfiguration": "android.att.release",
          "buildTarget": "my-app-e2e:build-android:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "open-cypress": {
      "command": "npx cypress open",
      "options": {
        "cwd": "apps/my-app-e2e"
      }
    },
    "run-cypress": {
      "command": "npx cypress run",
      "options": {
        "cwd": "apps/my-app-e2e",
        "browser": "edge"
      }
    },
    "install-web-test": {
      "command": "npx cypress install",
      "options": {
        "cwd": "apps/my-app-e2e"
      }
    },
    "launch-genymotion": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "sync-deps"
      ],
      "options": {
        "cwd": "apps/my-app-e2e",
        "command": "./detox/utils/startOrReuseGenymotion.sh"
      }
    },
    "stop-genymotion": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "sync-deps"
      ],
      "options": {
        "command": "gmsaas instances list -q | xargs -n1 gmsaas instances stop"
      }
    },
    "genycloud-test": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "sync-deps"
      ],
      "options": {
        "cwd": "apps/my-app-e2e",
        "command": "npx detox test --configuration android.genycloud.debug"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["my-app"]
}
