{
  "preset": "../../jest.preset",
  "rootDir": ".",
  "testMatch": [
  ],
  "testTimeout": 120000,
  "maxWorkers": 1,
  "verbose": true,
  "setupFilesAfterEnv": ["<rootDir>/test-setup.ts"],
  "transform": {
    "^.+\\.(ts|js|html)$": [
      "ts-jest",
      { "tsconfig": "<rootDir>/tsconfig.e2e.json" }
    ]
  }
}
